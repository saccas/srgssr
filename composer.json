{
    "name": "saccas/srgssr",
    "type": "typo3-cms-extension",
    "description": "Online media provider for srgssr.ch",
    "keywords": [
        "TYPO3",
        "extension",
        "online media helper",
        "video",
        "fal"
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "typo3/cms-core": "^9.5.17|| ^10.4.10 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "Saccas\\Srgssr\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saccas\\Srgssr\\Tests\\": "Tests"
        }
    },
    "extra": {
      "typo3/cms": {
         "extension-key": "srgssr"
      }
   }
}
