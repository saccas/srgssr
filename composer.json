{
    "name": "saccas/srgssr",
    "type": "typo3-cms-extension",
    "description": "Online media provider for srgssr.ch",
    "keywords": [
        "TYPO3",
        "extension",
        "online media helper",
        "video",
        "fal"
    ],
    "license": ["GPL-2.0-or-later"],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5.0|| ^10.2.99"
    },
    "autoload": {
        "psr-4": {
            "Saccas\\Srgssr\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saccas\\Srgssr\\Tests\\": "Tests"
        }
    },
    "replace": {
        "srgssr": "self.version",
        "typo3-ter/srgssr": "self.version"
    }
}
